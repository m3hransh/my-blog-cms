{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Projects",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "excerpt": {
      "type": "text"
    },
    "link": {
      "type": "string"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "startDate": {
      "type": "date"
    },
    "lastUpdate": {
      "type": "date"
    },
    "progress": {
      "type": "enumeration",
      "enum": [
        "Initialize",
        "Q1",
        "Q2",
        "Q3",
        "Finished"
      ]
    }
  }
}
