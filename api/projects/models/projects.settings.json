{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Projects",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "excerpt": {
      "type": "text"
    },
    "link": {
      "type": "string"
    },
    "coverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "startDate": {
      "type": "date"
    },
    "lastUpdate": {
      "type": "date"
    },
    "contributers": {
      "collection": "author",
      "via": "projects",
      "dominant": true
    },
    "tasks": {
      "type": "integer",
      "max": 100,
      "min": 0,
      "default": 10
    },
    "completedTasks": {
      "type": "integer",
      "max": 100,
      "min": 0,
      "default": 1
    }
  }
}
